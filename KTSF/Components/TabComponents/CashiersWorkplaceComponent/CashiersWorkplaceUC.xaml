             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KTSF.Components.TabComponents.CashiersWorkplaceComponent"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <Grid DockPanel.Dock="Right" Width="300"  Background="Red">
            
        </Grid>
        <Grid DockPanel.Dock="Top" Height="Auto" >
            <ContentControl Content="{Binding SearchComponent.Build}"/>
