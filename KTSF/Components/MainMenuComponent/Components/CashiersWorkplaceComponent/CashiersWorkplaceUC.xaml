<UserControl x:Class="KTSF.Components.MainMenuComponent.Components.CashiersWorkplaceComponent.CashiersWorkplaceUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KTSF.Components.MainMenuComponent.Components.CashiersWorkplaceComponent"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Components/MainMenuComponent/Components/CashiersWorkplaceComponent/CashiersWorkplaceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <StackPanel Orientation="Vertical">

            <ContentControl Content="{Binding SearchVM.SearchUC}"/>



            <TextBlock Text="Рабочее место кассира" FontSize="25"></TextBlock>
        </StackPanel>

    </Grid>
</UserControl>
