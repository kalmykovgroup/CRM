<Window x:Class="KTSF.Components.MainMenuComponent.Components.AdministrationComponent.Components.UsersPageComponents.Components.AddUserComponent.AddUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KTSF.Components.MainMenuComponent.Components.AdministrationComponent.Components.UsersPageComponents.Components.AddUserComponent"
        mc:Ignorable="d"
        Title="AddUserWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <DockPanel>

        <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Center" >

            <Button Command="{Binding CancelClickCommand}" Content="Отмена" Width="200" Height="40"></Button>
            <Button Command="{Binding SaveUserClickCommand}" Content="Добавить нового пользователя" Width="200" Height="40"></Button>
        </StackPanel>
        <Grid>
            <TextBlock Text="Все поля User для заполнения"></TextBlock>

        </Grid>
    </DockPanel>
</Window>
